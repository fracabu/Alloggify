# Gemini API Key (for OCR document extraction)
GEMINI_API_KEY=your_gemini_api_key_here

# Backend URL (Express server)
# Development: http://localhost:3001
# Production: Your Railway/Render URL
VITE_BACKEND_URL=http://localhost:3001

# Alloggiati Web Credentials (optional - for auto-fill in development)
# If not set, you'll need to enter them manually in the UI
VITE_ALLOGGIATI_UTENTE=your_username_here
VITE_ALLOGGIATI_PASSWORD=your_password_here
VITE_ALLOGGIATI_WSKEY=your_wskey_here
